[credential "https://code.vt.edu"]
	provider = generic
[credential "https://git.overleaf.com"]
	provider = generic
[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
[user]
	name = Caleb McIrvin
	email = calebmcirvin111@gmail.com
[credential "https://huggingface.co"]
	provider = generic
[credential]
	helper = 
	helper = /usr/local/share/gcm-core/git-credential-manager
	helper = store
[credential "https://dev.azure.com"]
	useHttpPath = true
[init]
	defaultBranch = main
[alias]
	a = "add"
	b = "branch"
	bd = "branch -D"
	c = "commit --verbose"
	ca = "commit --all --verbose"
	ce = "commit --amend --no-edit"
	cl = "clone"
	e = "restore"
	f = "diff"
	i = "init"
	l = "pull"
	ra = "remote add"
	rg = "remote get-url"
	rs = "reset --soft"
	rs = "reset"
	s = "status -s"
	u = "push"
	un = "push -u origin $(git rev-parse --abbrev-ref HEAD)"
	w = "switch"
	wc = "switch -c"
	wd = "switch --detach"
[core]
	pager = nvim -R
[color]
	pager = no
